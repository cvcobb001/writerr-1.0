{
  "name": "@writerr/track-edits",
  "version": "1.0.0",
  "description": "Track Edits plugin - Visual change tracking for Obsidian",
  "main": "main.js",
  "scripts": {
    "build": "rollup -c",
    "build:dev": "rollup -c --watch",
    "dev": "npm run build:dev",
    "clean": "rm -rf dist main.js manifest.json styles.css",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "obsidian": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.0",
    "rollup": "^3.29.4",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "obsidian": "*"
  }
}
